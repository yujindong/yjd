{
  "version": "independent",
  "changelog": {
    "repo": "yujindong/yjd",
    "cacheDir": ".changelog",
    "changelog": {
      "labels": {
        "feat": ":rocket: New Feature",
        "bug": ":bug: Bug Fix"
      }
